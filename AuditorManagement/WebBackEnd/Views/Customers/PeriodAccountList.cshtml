@{
    Layout = null;
}
<div class="container-fluid" data-ng-init="init()">

    <div class="row">
        <div class="col-sm-3">
            <div class="page-title-box">
                <h4>รอบบัญชี</h4>
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">บัญชี</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">รอบบัญชี</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">ภาพรวมทั้งหมด</a></li>
                </ol>
            </div>
            <div class="card">
                <div class="card-body card-body-table">
                    <div class="row  m-0">
                        <div class="col-md-6 col-sm-12">
                            <h5 class="card-title mt-1 px-0 font-weight-bold">
                                รายการรอบบัญชี
                            </h5>
                        </div>
                        <div class="col-md-6 col-sm-12 d-flex justify-content-end">
                            <button type="button" class="btn-dbd waves-effect "><i class="mdi mdi-magnify me-1"></i>ค้นหา</button>
                        </div>
                    </div>
                    <div class="row m-0">
                        <div class="col-md-12 py-3 ">
                            <div class="input-group">
                                <div class="input-group-text"><i class="mdi mdi-information" data-bs-toggle="tooltip" data-bs-placement="left" title="รหัสพนักงาน , ชื่อ"></i></div>
                                <input type="text" class="form-control" autocomplete="off" placeholder="พิมพ์เพื่อค้นหา...">
                            </div>
                        </div>
                    </div>
                    <div class="row m-0">
                        <div class="col-md-12 ">
                            <h5 class="card-title mt-1 px-0 ">
                                <span class="font-weight-bold">ชื่อลูกค้า : </span> {{CustomerData.Name}}
                            </h5>
                        </div>
                    </div>
                    <div class="row m-0">
                        <div class="col-md-12 ">
                            <div class="accordion accordion-flush" id="accordionFlushExample">
                                <div class="accordion-item" ng-repeat="year in Table">
                                    <h2 class="accordion-header" id="flush-headingOne">
                                        <button class="font-size-14 text-black accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne{{$index}}" aria-expanded="false" aria-controls="flush-collapseOne">
                                            ข้อมูลปี {{year.Year}}
                                        </button>
                                    </h2>
                                    <div id="flush-collapseOne{{$index}}" class="accordion-collapse collapse" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample" style="">
                                        <ol class="activity-feed mb-0">
                                            <li class="feed-item cursor-pointer" ng-repeat="values in year.Detail" ng-click="OnClickPeriod(values)">
                                                <div class="feed-item-list">
                                                    <span class="date">วันที่รอบบัญชี : {{values.StartDate}} - {{values.EndDate }}</span>
                                                    <span class="activity-text text-black">ชื่อรอบบัญชี : {{values.Name}}</span>
                                                </div>
                                            </li>
                                        </ol>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="col-xl-9">
