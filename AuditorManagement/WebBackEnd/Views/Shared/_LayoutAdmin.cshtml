                                <div>
                                    <i class="mdi mdi-chevron-down"></i>
                                </div>
                                <button type="button" class="btn header-item waves-effect btn-header" id="page-header-user-dropdown">
                                    <img class="rounded-circle header-profile-user" src="@Url.Content("~/")assets/images/icons/profile.png"
