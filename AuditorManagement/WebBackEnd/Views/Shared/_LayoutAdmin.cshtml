                                            <a ui-sref="admin-companydocument" ui-sref-active="active" class="dropdown-item">Company Document</a>
                                            <a ui-sref="admin-auditprogram" ui-sref-active="active" class="dropdown-item">Audit Program</a>
                                            <a asp-controller="Organizations" asp-action="CustomerAdd" class="dropdown-item">Audit Report</a>
                                        </div>
                                    </li>

