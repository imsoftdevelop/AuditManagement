@{
    Layout = null;
}


<div class="container-fluid" data-ng-init="initModal()">

    <div class="row">
        <div class="col-sm-4">
            <div class="page-title-box">
                <h4>FS Top</h4>
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">ตรวจสอบบัญชี</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">FS Top</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">ภาพรวมทั้งหมด</a></li>
                </ol>
            </div>
        </div>
        <div class="col-sm-8">
            <div class="d-flex flex-wrap gap-3 justify-content-end">
                <button type="button" class="btn btn-success waves-effect waves-light px-4 dropdown-toggle  mx-2 " ng-show="TablePeriod.IsAudit == 'No'" ng-click="OnClickBeforeAudit()">
                    <i class="mdi mdi-check-all mx-2"></i> เริ่มทำการตรวจสอบบัญชี
                </button>
            </div>
        </div>
    </div>
    <div class="row mb-1">
        <div class="col-xl-12">
            <div class="card mb-1">
                <div class="card-body card-body-table">
                    <div class="row">
                        <div class="col-md-3">
                            <h4 class="card-title mb-3">
                                <span class="badge-table"><i class="mdi mdi-calendar-account-outline mx-2"></i></span>
                                ข้อมูล FS Top
                            </h4>
                        </div>
                        <div class="col-md-9 d-flex gap-5 text-center">
                            <div class="p-1">
                                <h5 class="font-weight-bold">
                                    ชื่อลูกค้า
                                    <i class="mdi mdi-account-edit-outline me-1 text-search font-size-20 mx-2 cursor-pointer"
                                       ng-show="UserProfiles.CustomerIdActive == undefined || UserProfiles.CustomerIdActive == ''"
                                       data-bs-toggle="modal" data-bs-target="#ModalCustomer"></i>
                                </h5>
                                <span class="text-search">{{Search.SelectCustomer.Name}}  </span>
                            </div>
                            <div class="p-1">
                                <h5 class="font-weight-bold">
                                    รอบบัญชี
                                    <i class="mdi mdi-calendar-account-outline me-1 text-search font-size-20 mx-2 cursor-pointer"
                                       ng-click="OnClickChangePeriod()"></i>
                                </h5>
                                <span class="text-search">รอบบัญชีประจำปี  {{TablePeriod.Year}} </span>
                            </div>
                            <div class="p-1 mt-2">
                                <h5 class="font-weight-bold">วันที่รอบบัญชี</h5>
                                <span> {{TablePeriod.Date}} </span>
                            </div>
                            <div class="p-1 mt-2">
                                <h5 class="font-weight-bold">วันที่อัพโหลดล่าสุด</h5>
                                <span> {{TablePeriod.UploadDate}}</span>
                            </div>
                            <div class="p-1 mt-2">
                                <h5 class="font-weight-bold">เปรียบเทียบปี</h5>
                                <span>{{TablePeriod.MappingYear}}</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- container-fluid -->

