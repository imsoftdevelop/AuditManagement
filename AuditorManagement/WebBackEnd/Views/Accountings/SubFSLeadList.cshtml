                                                                        <span ng-show="subdetail.AdjustmentModel == 'PL' && subdetail.AdjustmentType == 'Debit'">{{subdetail.Debit | number : 2}}</span>
                                                                        <span ng-show="subdetail.AdjustmentModel == 'BS' || subdetail.AdjustmentType == 'Credit'">-</span>
