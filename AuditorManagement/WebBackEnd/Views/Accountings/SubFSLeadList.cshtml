@{
    Layout = null;
}

<div class="container-fluid" data-ng-init="initModal()">

    <div class="row">
        <div class="col-sm-5">
            <div class="page-title-box">
                <h4>Sub Lead</h4>
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">ตรวจสอบบัญชี</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">FS Lead</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Sub Lead</a></li>
                    <li class="breadcrumb-item"><a href="javascript: void(0);">ภาพรวมทั้งหมด</a></li>
                </ol>
            </div>
        </div>
        <div class="col-sm-7">
            <div class="d-flex flex-wrap gap-3 justify-content-end">
                <a class="btn btn-light waves-effect px-5 py-2" ui-sref="account-fstoplist">FS Top</a>
                <a class="btn btn-light waves-effect px-5 py-2" ui-sref="account-fsleadlist">FS Lead</a>
                <div class="btn-group" ng-show="TablePeriod.AccountSelect.Status == 'Wait' || TablePeriod.AccountSelect.Status == 'Back'">
                    <button type="button" class="btn btn-primary  waves-effect waves-light px-3 py-2 aksorn f-1-2" ng-click="OnClickSave()">บันทึกข้อมูล</button>
                    <button type="button" class="btn btn-primary   waves-effect waves-light px-2 py-1 dropdown-toggle dropdown-toggle-split" data-bs-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false">
                        <i class="mdi mdi-chevron-down"></i>
                    </button>
                    <div class="dropdown-menu" aria-labelledby="btnGroupVerticalDrop1" style="">
                        <a class="dropdown-item cursor-pointer" ng-click="OnClickSave('back')">บันทึก และย้อนกลับ</a>
                    </div>
                </div>
                <button type="button" class="btn btn-dark waves-effect waves-light px-4 dropdown-toggle  mx-2" ng-click="OnClickAddConfirm('Back')" ng-show="TablePeriod.AccountSelect.Status == 'Confirm'">
                    <i class="mdi mdi-comment-arrow-left-outline mx-2"></i>Back
                </button>
                <button type="button" class="btn btn-success waves-effect waves-light px-4 dropdown-toggle  mx-2 " ng-click="OnClickAddConfirm('Confirm')" ng-show="TablePeriod.AccountSelect.Status == 'Wait' || TablePeriod.AccountSelect.Status == 'Back'">
                    <i class="mdi mdi-check-all mx-2"></i>Confirm
                </button>
            </div>
        </div>
    </div>

    <div class="row mb-1">
        <div class="col-xl-12">
            <div class="card mb-1">
                <div class="card-body card-body-table">
                    <div class="row">
                        <div class="col-md-3">
                            <h4 class="card-title mb-3">
                                <span class="badge-table"><i class="mdi mdi-calendar-account-outline mx-2"></i></span>
                                ข้อมูล Sub Lead
                            </h4>
                        </div>
                        <div class="col-md-9 d-flex gap-5 text-center">
                            <div class="p-1">
                                <h5 class="font-weight-bold">
                                    ชื่อลูกค้า
                                    <i class="mdi mdi-account-edit-outline me-1 text-search font-size-20 mx-2 cursor-pointer"
                                       ng-show="UserProfiles.CustomerIdActive == undefined || UserProfiles.CustomerIdActive == ''"
                                       data-bs-toggle="modal" data-bs-target="#ModalCustomer"></i>
                                </h5>
                                <span class="text-search">{{Search.SelectCustomer.Name}}  </span>
                            </div>
                            <div class="p-1">
                                <h5 class="font-weight-bold">
                                    รอบบัญชี
                                </h5>
                                <span class="text-search">รอบบัญชีประจำปี  {{TablePeriod.Year}} </span>
                            </div>
                            <div class="p-1 ">
                                <h5 class="font-weight-bold">วันที่รอบบัญชี</h5>
                                <span> {{TablePeriod.Date}} </span>
                            </div>
                            <div class="p-1 ">
                                <h5 class="font-weight-bold">วันที่อัพโหลดล่าสุด</h5>
                                <span> {{TablePeriod.UploadDate}}</span>
                            </div>
                            <div class="p-1 ">
                                <h5 class="font-weight-bold">เปรียบเทียบปี</h5>
                                <span>{{TablePeriod.MappingYear}}</span>
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row  row-form">
                        <div class="col-md-3">
                            <h5 class="card-title mb-2 font-weight-bold h4-flex"><i data-v-4e8aa93a="" class="color-circle"></i>กลุ่มประเภท</h5>
                            <button type="button" class="mt-1 mx-2 btn btn-modal-add waves-effect waves-light" ng-click="OnClickAddReference()" ng-hide="TablePeriod.AccountSelect.Status == 'Confirm'"><i class="mdi mdi-link-variant-plus me-1"></i>อ้างอิงการตรวจสอบ</button>
                        </div>
                        <div class="col-md-9 d-flex gap-5 text-center">
                            <div class="p-1 text-center">
                                <h5 class="font-weight-bold ">Ref No.</h5>
                                <span class="text-search font-weight-bold cursor-pointer">
                                    {{TablePeriod.AccountSelect.AccountRefCode}}
                                </span>
                            </div>
                            <div class="p-1 text-center">
                                <h5 class="font-weight-bold ">
                                    Prepared by
                                </h5>
                                <span class="badge font-size-14 display-block" ng-class="{ 'bg-info' : TablePeriod.AccountSelect.PrepareStatus == 'Wait',
                                      'bg-success' : TablePeriod.AccountSelect.PrepareStatus == 'Confirm',
                                      'bg-danger' : TablePeriod.AccountSelect.PrepareStatus == 'Back'}">{{TablePeriod.AccountSelect.PrepareStatus}}</span>
                                <span class="display-block">{{TablePeriod.AccountSelect.PrepareData.FullName}}</span>
                                <span>{{TablePeriod.AccountSelect.PrepareDate}}</span>
                            </div>
                            <div class="p-1 text-center">
                                <h5 class="font-weight-bold ">
                                    Reveiwed by
                                </h5>
                                <span class="badge font-size-14 display-block" ng-class="{ 'bg-info' : TablePeriod.AccountSelect.ReveiwedStatus == 'Wait',
                                      'bg-success' : TablePeriod.AccountSelect.ReveiwedStatus == 'Confirm',
                                      'bg-danger' : TablePeriod.AccountSelect.ReveiwedStatus == 'Back'}">{{TablePeriod.AccountSelect.ReveiwedStatus}}</span>
                                <span class="display-block">{{TablePeriod.AccountSelect.ReveiwedData.FullName}}</span>
                                <span>{{TablePeriod.AccountSelect.ReveiwedDate}}</span>
                            </div>
                            <div class="p-1 text-center">
                                <h5 class="font-weight-bold ">
                                    Auditor by
                                </h5>
                                <span class="badge font-size-14 display-block" ng-class="{ 'bg-info' : TablePeriod.AccountSelect.AuditorStatus == 'Wait',
                                      'bg-success' : TablePeriod.AccountSelect.AuditorStatus == 'Confirm',
                                      'bg-danger' : TablePeriod.AccountSelect.AuditorStatus == 'Back'}">{{TablePeriod.AccountSelect.AuditorStatus}}</span>
                                <span class="display-block">{{TablePeriod.AccountSelect.AuditorData.FullName}}</span>
                                <span>{{TablePeriod.AccountSelect.AuditorDate}}</span>
                            </div>
                            <div class="p-1 text-center">
                                <h5 class="font-weight-bold ">
                                    Ref No. Verify
                                </h5>
                                <span class="text-search font-weight-bold cursor-pointer">
                                    <a ng-click="OnClickReference()">
                                        <i class="mdi mdi-magnify font-size-18" ng-show="TablePeriod.AccountSelect.ReferenceVerify.AuditAccount.AccountRefCode != '-'"></i> {{TablePeriod.AccountSelect.ReferenceVerify.AuditAccount.AccountRefCode}}
                                    </a>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- container-fluid -->

    <div class="row">

        <div class="col-xl-12">
            <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item active" data-bs-toggle="tab" href="#lead" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="fas fa-home"></i></span>
                        <span class="d-none d-sm-block">Sub Lead</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#adjustment" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Adjustment</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#document" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Document</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#conclusion" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Audit Conclusion</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#comment" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Audit Comment</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#issue" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Audit Issue</span>
                    </a>
                </li>
                <li class="nav-item" data-bs-toggle="tab" href="#timeline" role="tab">
                    <a class="nav-link">
                        <span class="d-block d-sm-none"><i class="far fa-user"></i></span>
                        <span class="d-none d-sm-block">Timeline</span>
                    </a>
                </li>
            </ul>
            <div class="tab-content">
                <div class=" tab-pane card active" id="lead" role="tabpanel">
                    <div class="card-body card-body-form card-accordion p-0">
                        <div class="table-rep-plugin">
                            <div class="table-responsive mb-0" data-pattern="priority-columns">
                                <table id="tech-companies-1" class="table-data-mulH-padding   table-striped">
                                    <thead>
                                        <tr class="headerRow">
                                            <th width="2%" rowspan="2"></th>
                                            <th width="23%" rowspan="2" class="text-center">A/C Code - Name</th>
                                            <th width="10%" rowspan="2" class="text-center">Sub Group</th>
                                            <th width="10%" rowspan="2" class="text-center">Sub Lead</th>
                                            <th width="10%" class="text-center" rowspan="2">Current Year</th>
                                            <th width="16%" colspan="2" class="text-center">Adj / Rje</th>
                                            <th width="10%" class="text-center" rowspan="2">Per Audit</th>
                                            <th width="10%" class="text-center" rowspan="2">Last Year</th>
                                            <th width="14%" class="text-center" colspan="2">Movement</th>
                                        </tr>
                                        <tr class="headerRow">
                                            <th width="8%" class="text-center">Dr</th>
                                            <th width="8%" class="text-center">Cr</th>
                                            <th width="8%" class="text-center">In (de)</th>
                                            <th width="6%" class="text-center">%</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="values in TablePeriod.AccountTable" ng-show="TablePeriod.AccountTable != undefined && TablePeriod.AccountTable.length > 0">
                                            <td width="2%">{{values.Seq}}</td>
                                            <td width="23%">{{values.Code}} - {{values.Name}}</td>
                                            <td width="10%">
                                                <div class="form-input " ng-show="TablePeriod.AccountSelect.AccountRefCode == values.AccountRefCode">
                                                    <select select2 class="dropdown2 form-control" id="exampleFormControlSelect11" ng-disabled="TablePeriod.AccountSelect.Status == 'Confirm'" 
                                                            ng-options="item as item.SubFSGroup.Code + ' - ' + item.SubFSGroup.Name for item in Parameter.SubFSGroup"
                                                            ng-model="values.SelectSubFSGroup">
                                                        <option value="" class="text-muted" selected style="display:none">กรุณาเลือก</option>
                                                    </select>
                                                </div>
                                            </td>
                                            <td width="10%" class="text-center text-search">{{values.AccountRefCode}}</td>
                                            <td width="10%" class="text-right">{{values.Current | number : 2}}</td>
                                            <td width="8%" class="text-right text-danger">{{values.Debit| number : 2}}</td>
                                            <td width="8%" class="text-right text-danger">{{values.Credit| number : 2}}</td>
                                            <td width="10%" class="text-right">{{values.Audit| number : 2}}</td>
                                            <td width="10%" class="text-right">{{values.Previous| number : 2}}</td>
                                            <td width="8%" class="text-right">{{values.Move| number : 2}}</td>
                                            <td width="6%" class="text-right">{{values.Percent| number : 2}}%</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <th></th>
                                            <td class="text-right font-weight-bold" colspan="3">รวม</td>
                                            <td class="text-right font-weight-bold">{{TablePeriod.Footer.Current | number : 2}}</td>
                                            <td class="text-right font-weight-bold text-danger">{{TablePeriod.Footer.Debit | number : 2}}</td>
                                            <td class="text-right font-weight-bold text-danger">{{TablePeriod.Footer.Credit | number : 2}}</td>
                                            <td class="text-right font-weight-bold">{{TablePeriod.Footer.Audit | number : 2}}</td>
                                            <td class="text-right font-weight-bold">{{TablePeriod.Footer.Previous | number : 2}}</td>
                                            <td class="text-right font-weight-bold">{{TablePeriod.Footer.Move | number : 2}}</td>
                                            <td class="text-right font-weight-bold">{{TablePeriod.Footer.Percent | number : 2}}%</td>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        <tr class="text-center " ng-show="TablePeriod.AccountTable == undefined || TablePeriod.AccountTable.length <= 0">
                                            <td colspan="9" class="empty">
                                                ไม่พบรายการ
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                <hr />
                            </div>
                        </div>
                    </div>
                </div>
                <div class=" tab-pane card " id="adjustment" role="tabpanel">
                    <div class="card-body card-body-form card-accordion">
                        <div class="row">
                            <div class="col-md-6">
                                <h4 class="card-title mb-3">
                                    <span class="badge-table"><i class="mdi mdi-calendar-account-outline mx-2"></i></span>
                                    Adjustment
                                </h4>
                            </div>
                            <div class="col-md-6 text-right">
                                <button type="button" class="btn btn-outline-dark waves-effect mx-2 py-2 px-3" ng-click="OnClickAddAdjustment()" ng-hide="TablePeriod.AccountSelect.Status == 'Confirm'">
                                    <i class="mdi mdi-plus mx-2"></i> Adjustment
                                </button>
                            </div>
                        </div>

